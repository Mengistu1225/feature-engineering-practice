   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Sample DataFrame\n",
    "data = {'Age': [22, np.nan, 24, np.nan, 28]}\n",
    "df = pd.DataFrame(data)\n",
    "print(df)\n",
    "# Apply arbitrary imputation with the value 999 for missing 'Age' values\n",
    "df['Age_Arbitrary_Imputed'] = df['Age'].fillna(999)\n",
    "\n",
    "print(df)\n"
   ]
